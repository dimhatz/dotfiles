        //     "after": [ "<ESC>", "A"]
        // },
        // {
        //     "before": [ "c-j" ],
        //     "after": [ "<ESC>", "I"]
        // }
    ],
    "workbench.startupEditor": "newUntitledFile",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
	"mongo.shell.path": "c:\\mongodb-win32-x86_64-2012plus-4.2.0\\bin\\mongo.exe"
